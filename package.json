{
  "name": "react-starter",
  "version": "1.1.0",
  "license": "MIT",
  "author": "wwwenjie",
  "description": "React Starter",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext .ts,.tsx",
    "test:prettier": "prettier \"**/*.{ts,tsx,md,css,html,json}\" --list-different",
    "test:css": "stylelint \"**/*.css\"",
    "test:build": "run-p build",
    "test:unit": "jest --collectCoverage",
    "watch": "run-p watch:*",
    "watch:test": "jest --watch",
    "cov": "run-s test:unit && open-cli coverage/lcov-report/index.html",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"**/*.{ts,tsx,md,css,html,json}\" --write",
    "fix:lint": "eslint src --ext .ts,.tsx --fix",
    "release": "standard-version"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "i18next": "^21.5.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "rooks": "^5.8.0",
    "swr": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-svgr": "^0.6.0",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
