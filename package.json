{
  "name": "react-starter",
  "version": "1.4.0",
  "license": "MIT",
  "author": "wwwenjie",
  "description": "React Starter",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext .ts,.tsx",
    "test:prettier": "prettier \"**/*.{ts,tsx,md,css,html,json}\" --list-different",
    "test:css": "stylelint \"**/*.css\"",
    "test:unit": "vitest run",
    "test:build": "run-p build",
    "watch": "run-p watch:*",
    "watch:test": "vitest watch",
    "cov": "vitest run --coverage",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"**/*.{ts,tsx,md,css,html,json}\" --write",
    "fix:lint": "eslint src --ext .ts,.tsx --fix",
    "release": "standard-version"
  },
  "dependencies": {
    "antd": "^5.6.1",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.8",
    "i18next": "^22.5.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.12.0",
    "rooks": "^7.14.1",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.32.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
